{
  "name": "imdhemy/alexander",
  "description": "Wordpress theme for building your online business",
  "type": "project",
  "license": "GPL2",
  "authors": [
    {
      "name": "imdhemy",
      "email": "imdhemy@gmail.com"
    }
  ],
  "require": {
    "imdhemy/alexander-http": "@dev",
    "imdhemy/alexander-helpers": "@dev",
    "symfony/console": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Alexander\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": {
    "alexander-http": {
      "type": "path",
      "url": "components/alexander-http",
      "options": {
        "symlink": true
      }
    },
    "alexander-helpers": {
      "type": "path",
      "url": "components/alexander-helpers",
      "options": {
        "symlink": true
      }
    }
  }
}
